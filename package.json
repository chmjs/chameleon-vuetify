{
  "name": "chameleon-vuetify",
  "version": "1.0.0",
  "description": "Chameleon Vuetify Interpreter",
  "author": "Jadranko Dragoje <dragoje.jadranko@nsoft.com>",
  "contributors": [
    "Snježana Drežnjak <dreznjak.snjezana@nsoft.com>",
    "Lana Džidić <dzidic.lana@nsoft.com>"
  ],
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js --open --hot",
    "unit": "jest --config tests/unit/jest.conf.js",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src tests/unit/specs",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config build/config.js --progress --hide-modules",
    "build:docs": "node build/docs.js",
    "deploy": "npm run build && netlifyctl deploy",
    "zip-bundle": "node ./build/bundle.js",
    "deploy-bundle": "node ./build/deploy.js",
    "bundle": "npm run build && npm run zip-bundle"
  },
  "main": "dist/chameleon-vuetify.min.js",
  "dependencies": {
    "@nsoft/chameleon-sdk": "^1.0.36",
    "@nsoft/chameleon-notation": "^1.0.15",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "quill": "^1.3.6",
    "uuid": "^3.2.1",
    "validator": "^9.4.1",
    "vue": "^2.5.16",
    "vue-jsoneditor": "^1.0.13",
    "vuetify": "^1.0.16"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.15",
    "autoprefixer": "^7.2.6",
    "axios": "^0.17.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^23.0.0-beta.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
