{
  "name": "chameleon-vuetify",
  "version": "1.0.0",
  "description": "Chameleon Vuetify Interpreter",
  "author": "Jadranko Dragoje <dragoje.jadranko@nsoft.com>",
  "contributors": [
    "Snježana Drežnjak <dreznjak.snjezana@nsoft.com>",
    "Lana Džidić <dzidic.lana@nsoft.com>"
  ],
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js --open --hot",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config build/config.js --progress --hide-modules",
    "build:docs": "node build/docs.js",
    "deploy": "npm run build && netlifyctl deploy"
  },
  "main": "dist/chameleon-vuetify.min.js",
  "dependencies": {
    "chameleon-notation": "^1.0.7",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "quill": "^1.3.5",
    "uuid": "^3.2.1",
    "validator": "^9.4.1",
    "vue": "^2.5.13",
    "vue-jsoneditor": "^1.0.11",
    "vuetify": "^1.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "axios": "^0.17.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.1",
    "copy-webpack-plugin": "^4.5.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.13",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^3.2.0",
    "node-notifier": "^5.2.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "optimize-js-plugin": "0.0.4",
    "ora": "^1.4.0",
